{% macro elements(childrens) %}
    {% for key, children in childrens %}
        <{{ key }}>
        {% if (children is iterable) %}
            {{ _self.elements(children) }}
        {% else %}
            {{ children }}
        {% endif %}
        </{{ key }}>
    {% endfor %}
{% endmacro %}

